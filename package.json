{
  "name": "canvas-dojo",
  "publisher": "ArthurKim",
  "displayName": "canvas dojo",
  "description": "snippets and plugins for html5 canvas development",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/znxkznxk1030/canvas-dojo"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:canvas-dojo.start",
    "onCommand:canvas-dojo.doRefactor",
    "onView:canvas-dojo.canvasView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "canvas-dojo.canvasView",
          "name": "Canvas Preview"
        }
      ]
    },
    "commands": [
      {
        "command": "canvas-dojo.start",
        "title": "Canvas Preview"
      },
      {
        "command": "canvas-dojo.doRefactor",
        "title": "Canvas Preview Update"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/canvas-snippet.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier \"src/**/*.{js,ts,html}\" --write"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
